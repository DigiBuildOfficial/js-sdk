{
  "name": "@digibuildofficial/js-sdk",
  "version": "2.3.3",
  "description": "DigiBuild's fork of Procore's API SDK.",
  "main": "dist/index.ts",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/DigiBuildOfficial/js-sdk",
  "repository": "git://github.com/DigiBuildOfficial/js-sdk.git",
  "scripts": {
    "prepublish": "npm run build",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "test:watch": "mocha -r ts-node/register test/**/*.spec.ts -w --watch-extensions ts,tsx",
    "watch": "rm -rf dist/ && tsc --watch",
    "build": "rm -rf dist/ && tsc",
    "docs": "typedoc --includeDeclarations --excludeExternals --readme README.md --target es5 --out docs dist/*.ts",
    "endpoints": "node-procore-endpoints lib"
  },
  "author": "Procore Tech <insights@procore.com> (http://procore.com)",
  "license": "ISC",
  "dependencies": {
    "@procore/js-sdk-endpoints": "1.9.1",
    "isomorphic-fetch": "^2.2.1",
    "qs": "^6.3.0",
    "ramda": "^0.23.0",
    "string": "^3.3.3"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/core-js": "^0.9.35",
    "@types/fetch-mock": "^5.8.0",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/mocha": "^2.2.38",
    "@types/qs": "^6.2.30",
    "@types/ramda": "^0.0.3",
    "@types/sinon": "^2.3.2",
    "@types/string": "^0.0.28",
    "chai": "^3.5.0",
    "fetch-mock": "^5.9.3",
    "mocha": "^5.2.0",
    "sinon": "^2.3.8",
    "ts-node": "^2.0.0",
    "typedoc": "^0.5.6",
    "typescript": "^3.9.2"
  }
}
